{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cno-lite-vimeo/cno-plugin-lite-vimeo-block",
	"version": "1.0.0",
	"title": "CNO Lite Vimeo Block",
	"category": "embed",
	"icon": "video-alt3",
	"keywords": [ "vimeo", "video", "embed", "lite-vimeo" ],
	"description": "A block that uses lite-vimeo to load Vimeo videos in a lightweight, performant way.",
	"example": {},
	"supports": {
		"html": false,
		"align": true,
		"dimensions": {
			"minHeight": true
		},
		"customClassName": true,
		"spacing": {
			"margin": [ "top", "bottom" ]
		}
	},
	"attributes": {
		"isUnlisted": {
			"type": "boolean",
			"default": false
		},
		"customThumbnailURL": {
			"type": "string",
			"default": ""
		},
		"videoID": {
			"type": "string",
			"default": "",
			"required": true
		},
		"videoTitle": {
			"type": "string",
			"default": "Video"
		},
		"loop": {
			"type": "boolean",
			"default": false
		},
		"disableTracking": {
			"type": "boolean",
			"default": false
		},
		"videoStartAt": {
			"type": "number",
			"default": ""
		}
	},
	"script": "file:./lite-vimeo/index.ts",
	"editorScript": "file:./index.tsx"
}
